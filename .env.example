# Google AI Studio Configuration
GOOGLE_AI_API_KEY=

# FastAPI Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_DEBUG=False

# Repomix Configuration
REPOMIX_INCLUDE_PATTERNS=*.java,*.py,*.md,*.txt
REPOMIX_EXCLUDE_PATTERNS=.git/*,node_modules/*,__pycache__/*,*.class

# Application Configuration
INCLUDE_REPO_CONTEXT_BY_DEFAULT=True
LOG_LEVEL=INFO
REPO_PATH=.

# Model Names
MODEL_NAME_ONE=gemini-2.5-flash
MODEL_NAME_TWO=gemma-3-27b-it

# Token budget por request de entrada (incluye system + prompt + contexto repo)
MAX_INPUT_TOKENS=10000
